# KRIZZY OPS v3 Environment Configuration
# Copy this file to .env and fill in your actual values

# Airtable Configuration
AIRTABLE_API_KEY=your_airtable_api_key_here
AIRTABLE_BASE_ID=your_airtable_base_id_here

# Discord Webhooks
DISCORD_WEBHOOK_OPS=https://discord.com/api/webhooks/your/ops/webhook
DISCORD_WEBHOOK_ERRORS=https://discord.com/api/webhooks/your/errors/webhook

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_MESSAGING_SERVICE_SID=your_messaging_service_sid

# Government Contracting Configuration
NAICS_WHITELIST=541511,541512,541519,541330,541990
UEI=your_unique_entity_identifier
CAGE_CODE=your_cage_code
FPDS_ATOM_FEED=https://www.fpds.gov/ezsearch/FEEDS/ATOM?FEEDNAME=PUBLIC&q=ACTIVE_DATE:[NOW-7DAYS+TO+NOW]
SAM_SEARCH_API=https://api.sam.gov/opportunities/v2/search

# Application Configuration
PORT=8080
FLASK_ENV=production
PROXY_LIST=proxy1:port,proxy2:port,proxy3:port

# Optional: LLM Configuration (for content enrichment)
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GEMINI_API_KEY=your_gemini_api_key